<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
    	<report auto="False" menu="False" id="sale.report_sale_order" model="sale.order" name="sale.order"
                rml="sale/report/sale_order.rml" string="Quotation / Order"
                usage="default"/>
		
		<report menu="False" id="stock.report_picking_list_out" model="stock.picking.out" name="stock.picking.list.out" string="Delivery Slip" rml="stock/report/picking.rml"/>
		
		<report
            auto="False"
            menu="False"
            id="account.account_invoices"
            model="account.invoice"
            name="account.invoice"
            rml="account/report/account_print_invoice.rml"
            string="Invoices"
