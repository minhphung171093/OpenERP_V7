            attachment_use="True"
            usage="default"
            />
		
        <report auto="False" menu="True" id="report_quotation_sample" model="sale.order" name="quotation_sample"
                rml="tgb_bez_balloons_report/report/quotation_sample.rml" string="Quotation Sample"
                usage="default" header="False"/>
		
		<report auto="False" menu="True" id="report_delivery_order_sample" model="stock.picking.out" name="delivery_order_sample"
                rml="tgb_bez_balloons_report/report/delivery_order_sample.rml" string="Delivery Order Sample"
                usage="default" header="False"/>
                
        <report auto="False" menu="False" id="report_cash_sales_sample" model="account.invoice" name="cash_sales_sample"
                rml="tgb_bez_balloons_report/report/cash_sales_sample.rml" string="Cash Sales Sample"
                usage="default" header="False"/>
                
        <report auto="False" menu="True" id="report_tax_invoice_sample" model="account.invoice" name="tax_invoice_sample"
                rml="tgb_bez_balloons_report/report/tax_invoice_sample.rml" string="Tax Invoice Sample"
                usage="default" header="False"/>
		
    </data>
</openerp>
