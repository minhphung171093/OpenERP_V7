<?xml version="1.0"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="audionote_make_sale_deposit_form">
            <field name="name">AudioNote.add.sale.deposit.form</field>
            <field name="model">add.sale.deposit.wiz</field>
            <field name="arch" type="xml">
                <form string="Tracking Number" version="7.0">
                    <group col="4">
                        <separator string="Make new deposit" colspan="4"/>
                        <field name="amount"/>
                        <field name="journal_id"/>
                        <field name="period_id"/>
                        <field name="cheque_no"/>
                        <newline/>
                        <button name="confirm" string="Confirm" type="object"/>
                    </group>
                </form>
            </field>
        </record>

    </data>
</openerp>
