<?xml version="1.0"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="tgb_construction_stock_move_form_view">
        <field name="name">stock.move.form</field>
        <field name="model">stock.move</field>
        <field name="arch" type="xml">
            <form string="stock_move" version="7.0">
            <sheet>
                <group>
                    <field name="type"/>
                    <field name="remarks_description"/>
                    <field name="source_voucher_no"/>
                    <field name="product_uom_id"/>
                    <field name="pack size"/>
                    <field name="no_of_pack"/>
                    <field name="qty_req"/>
                    <field name="no_of_pack_req"/>
                    <field name="ost_qty"/>
                    <field name="complete"/>
                    <field name="serial_no"/>
                    
                </group>
                   <notebook>
                        
                    </notebook>
                   </sheet>
             </form>
         </field>
    </record>
    
        <record model="ir.ui.view" id="tgb_construction_stock_move_tree_view">
        <field name="name">stock.move.form</field>
        <field name="model">stock.move</field>
        <field name="arch" type="xml">
            <tree string="stock_move">
                <field name="type"/>
                <field name="remarks_description"/>
                <field name="source_voucher_no"/>
                <field name="product_uom_id"/>
                <field name="pack size"/>
                <field name="no_of_pack"/>
                <field name="qty_req"/>
                <field name="no_of_pack_req"/>
                <field name="ost_qty"/>
                <field name="complete"/>
                <field name="serial_no"/>
            </tree>
        </field>
    </record>
        <record id="tgb_construction_stock_move_action" model="ir.actions.act_window">
            <field name="name">stock_move</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">stock.move</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{}</field>
        </record>
    
     </data>
</openerp>
